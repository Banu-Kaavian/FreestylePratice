<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.employeemanagement.controller.View1">
    <Page id="page" title="{i18n>title}">
        <content>
            <Table noDataText="Drop column list items here and columns in the area above" id="table0" items="{MainModel>/Employee}">
                <columns>
                    <Column id="column3">
                        <header>
                            <Label text="ID" id="label3" />
                        </header>
                    </Column>
                    <Column id="column0">
                        <header>
                            <Label text="Name" id="label0" />
                        </header>
                    </Column>
                    <Column id="column1">
                        <header>
                            <Label text="Email" id="label1" />
                        </header>
                    </Column>
                    <Column id="column2">
                        <header>
                            <Label text="Department" id="label2" />
                        </header>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem type="Active" id="item0">
                        <cells>
                            <Text text="{MainModel>ID}" id="text5"/>
                            <Text text="{MainModel>Name}" id="text2" />
                            <Text text="{MainModel>Email}" id="text3" />
                            <Text text="{MainModel>Department}" id="text4" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
